{
    "name": "slifin/edn",
    "description": "Extensible Data Notation Parser.",
    "keywords": ["edn"],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "nikic/phlexy": "^0.2"
    },
    "require-dev": {
        "shaunxcode/edn-tests": "dev-master",
        "rhumsaa/uuid": "~2.4"
    },
    "suggest": {
        "rhumsaa/uuid": "If you want to use UUID tags."
    },
    "autoload": {
        "files": ["src/datastructure.php", "src/parser.php", "src/encoder.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "shaunxcode/edn-tests",
                "version": "dev-master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/shaunxcode/edn-tests",
                    "reference": "master"
                }
            }
        }
    ]
}
